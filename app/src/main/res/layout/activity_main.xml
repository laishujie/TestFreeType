<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_create_Sdf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sdf纹理描边"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/btn_textFont"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <Button
        android:id="@+id/btn_inset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="字体纹理管理"
        app:layout_constraintEnd_toEndOf="@+id/btn_create_Sdf"
        app:layout_constraintStart_toStartOf="@+id/btn_create_Sdf"
        app:layout_constraintTop_toBottomOf="@+id/btn_create_Sdf" />

</androidx.constraintlayout.widget.ConstraintLayout>